<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="739.0" prefWidth="1194.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sebe3012.servercontroller.gui.FrameHandler">
   <top>
      <MenuBar fx:id="mBar" BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="Datei">
               <items>
                  <MenuItem fx:id="saveItem" mnemonicParsing="false" onAction="#onSaveItemClicked" text="Einstellungen speichern" />
                  <MenuItem fx:id="openItem" mnemonicParsing="false" onAction="#onOpenItemClicked" text="Einstellungen laden" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Server">
              <items>
                <MenuItem fx:id="addServer" mnemonicParsing="false" onAction="#onAddServerItemClicked" text="Server hinzufügen" />
                  <MenuItem mnemonicParsing="false" onAction="#onServerEditItemClicked" text="Server bearbeiten" />
                  <MenuItem mnemonicParsing="false" onAction="#onServerRemoveItemClicked" text="Server entfernen" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Hilfe">
              <items>
                <MenuItem fx:id="over" mnemonicParsing="false" onAction="#onOverItemClicked" text="Über" />
              </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
   <bottom>
      <Label fx:id="credits" alignment="CENTER" text="Alpha 0.1.3" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <VBox fx:id="vBox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <CheckBox mnemonicParsing="false" text="CheckBox" />
            <CheckBox mnemonicParsing="false" text="CheckBox" />
            <CheckBox mnemonicParsing="false" text="CheckBox" />
            <Separator prefWidth="200.0" />
            <Button mnemonicParsing="false" text="Button" />
         </children>
      </VBox>
   </right>
   <center>
      <TabPane fx:id="main" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <VBox prefHeight="697.0" prefWidth="272.0" BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="lView" prefHeight="662.0" prefWidth="209.0" />
            <PieChart fx:id="ramUsed" />
            <PieChart fx:id="ramTotal" />
            <PieChart fx:id="cpu" />
         </children>
      </VBox>
   </left>
</BorderPane>
